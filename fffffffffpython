from decimal import Decimal
import math

s = '0.00000001'
pow = int(-math.log10(Decimal(s))-8)
matic = s if (pow==0) else s[:pow]
print(matic)
print(pow)
p = int(-math.log10(Decimal(matic)))
purchase_price = Decimal('0.00000066')
print(('{0:.' + str(p) + 'f}').format(purchase_price.quantize(Decimal(matic))))
print(purchase_price.quantize(Decimal(matic)))
